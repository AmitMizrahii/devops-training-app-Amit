
datasource db {
  provider = "postgresql"
  url      = env("PG_URL")
}

enum Role {
  USER
  ADMIN
}

model users {
  id String @id @default(uuid())
  email String  @unique
  name  String?
  phone String? @unique
  password String
  role Role @default(USER)
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
}



